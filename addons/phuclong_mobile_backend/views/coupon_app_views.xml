<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <data>
        <record id="coupon_app_tree" model="ir.ui.view">
            <field name="name">coupon_app_tree</field>
            <field name="model">coupon.app</field>
            <field name="arch" type="xml">
                <tree create="0" edit="0">
                    <field name="name" />
                    <field name="type"/>
                    <field name="publish_date"/>
                    <field name="effective_date_from"/>
                    <field name="effective_date_to"/>
                    <field name="partner_id"/>
                    <field name="state"/>
                </tree>
            </field>
        </record>
        <record id="coupon_app_act" model="ir.actions.act_window">
            <field name="name">Coupon APP</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">coupon.app</field>
            <field name="view_mode">tree,form</field>
        </record>
    </data>
</odoo>