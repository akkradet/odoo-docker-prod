<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <data>
        <record id="res_users_inherit_mobile" model="ir.ui.view">
            <field name="name">res_users_inherit_mobile</field>
            <field name="model">res.users</field>
            <field name="inherit_id" ref="phuclong_pos_theme.view_users_form_add_required_emp_card" />
            <field name="arch" type="xml">
                <xpath expr="//field[@name='required_emp_card']" position="after">
                    <field name="use_for_website"/>
                </xpath>
            </field>
        </record>
    </data>
</odoo>